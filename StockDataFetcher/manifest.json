{
  "name": "Hello World!",
  "description": "My first Chrome.",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["back.js"]
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action":{
    "default_icon":"icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
  
}